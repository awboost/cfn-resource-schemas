{
  "name": "@awboost/cfn-codegen",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "cfn-codegen": "./lib/cli/main.js"
  },
  "type": "module",
  "author": {
    "email": "gordonmleigh@gmail.com",
    "name": "Gordon Leigh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/awboost/cfn-resource-schemas"
  },
  "license": "MIT",
  "exports": {
    "./types": {
      "types": "./lib/types.d.ts",
      "default": "./lib/types.js"
    }
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "lib/**/*.map"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile && npm run lint",
    "clean": "rm -rf lib/",
    "compile": "tsc",
    "lint": "eslint src/ --ext=ts"
  },
  "dependencies": {
    "@awboost/cfn-resource-schemas": "^0.1.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "json-canonicalize": "^1.0.6",
    "json-pointer": "^0.6.2",
    "prettier": "*",
    "typescript": "^5"
  }
}
